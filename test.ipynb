   "source": [
    "# 使用for循环计算1000以内的素数和，加上进度条\n",
    "sum = 0\n",
    "for i in tqdm(range(2, 1000)):\n",
    "    for j in range(2, i):\n",
    "        if i % j == 0:\n",
    "            break\n",
    "    else:\n",
    "        sum += i\n",
    "print(sum)"
   ]
